{
  "name": "cadence-js",
  "version": "0.0.3",
  "repository": "https://github.com/timothee-durand/cadence",
  "description": "A typescript library to create music with code.",
  "main": "src/index.ts",
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
