{
  "name": "cadence-js",
  "version": "0.0.2",
  "repository": "https://github.com/timothee-durand/cadence",
  "description": "A typescript library to create music with code.",
  "main": "src/index.ts",
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "generate:playground": "tsc --project tsconfig.playground-declaration.json && swc -s --config-file .swcrc -o ../playground/public/cadence.js ./src"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.95",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
