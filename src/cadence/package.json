  },
  "keywords": ["music", "typescript", "sample"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.5.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.95",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
